<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="player.manager" class="AppBundle\Service\PlayerManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="logger"/>
        </service>

        <service id="club.manager" class="AppBundle\Service\ClubManager">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="logger"/>
        </service>

        <service id="players_type.service" class="AppBundle\Form\Type\PlayersType">
            <argument type="service" id="player.manager" />
            <tag name="form.type" alias="playerSelect"/>
        </service>
    </services>
</container>




